{
  "name": "mikron-ia/rpg-hub-frontend-static",
  "version": "0.7.0",
  "authors": [
    {
      "name": "Mikron",
      "email": "mikronia@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mikron\\HubFront\\": "src/"
    }
  },
  "require": {
    "php": ">=5.5",
    "ext-mbstring": "*",
    "silex/silex": "~1.3",
    "doctrine/dbal": "^2.5",
    "twig/twig": "*",
    "symfony/config": "^2.7",
    "symfony/security": "~2.7",
    "symfony/translation": "~2.7",
    "symfony/twig-bridge": "*",
    "symfony/yaml": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "scripts": {
    "test": "php vendor/phpunit/phpunit/phpunit --testdox --configuration ./tests/phpunit.xml --coverage-clover build/logs/clover.xml"
  }
}
